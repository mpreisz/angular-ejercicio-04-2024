  },
  "[md]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[azure-pipelines]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "markdownlint.config": {
      "default": true,
      "MD030": {
          "ol_multi": 1,
          "ol_single": 1,
          "ul_multi": 3,
          "ul_single": 3
      }
  },
  "eslint.format.enable": true,
